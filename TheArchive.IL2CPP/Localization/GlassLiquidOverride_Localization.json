      "FSDescription": {
        "English": "The resolution used to render the liquid effects.\n\nIncreasing this settings might decrease performance on weaker hardware!",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "用于渲染液体效果的分辨率。\n\n增加此设置可能会降低性能，特别是在性能较低的硬件上！"
      }
    },
    "TheArchive.Features.Accessibility.GlassLiquidOverride+GlassLiquidOverrideSettings.ApplyDisinfectButton": {
      "FSDisplayName": {
        "English": "Clean Screen",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "清理屏幕"
      },
      "FSDescription": {
        "English": "Applies the Disinfect Station visuals to clear any other liquids.\n\n(Might fix bad cases of liquid bug if spammed a bunch!)",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "应用消毒站视觉效果以清除其他液体。\n\n（如果频繁使用可能修复液体BUG的严重情况！)"
      },
