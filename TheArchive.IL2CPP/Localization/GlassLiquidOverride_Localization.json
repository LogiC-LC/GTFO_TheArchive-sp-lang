    "TheArchive.Features.Accessibility.GlassLiquidOverride.Description": {
      "FDescription": {
        "English": "Adjust the games \"Glass Liquid System\"\nThe thing that renders the blood splatters etc on your visor.",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "调整游戏的玻璃液体系统，即渲染在面罩上的血迹等效果。"
      }
    },
    "TheArchive.Features.Accessibility.GlassLiquidOverride+GlassLiquidOverrideSettings.EnableGlassLiquidSystem": {
      "FSDisplayName": {
        "English": "Enable Liquid System",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "启用液体系统"
      },
      "FSDescription": {
        "English": "Turning this off stops the Glass Liquid System from updating.\n\nMake sure that nothing's left on your visor or else it'll be stuck there until you re-enable this setting.",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "关闭此选项会停止玻璃液体系统的更新。\n\n确保面罩上没有残留物，否则它会一直停留在那里，直到重新启用此设置。"
      }
    },
    "TheArchive.Features.Accessibility.GlassLiquidOverride+GlassLiquidOverrideSettings.Quality": {
