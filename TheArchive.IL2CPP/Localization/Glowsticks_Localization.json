{
  "FeatureSettingsTexts": {
    "TheArchive.Features.Cosmetic.Glowsticks.Name": {
      "FName": {
        "English": "Glowsticks!",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "荧光棒！"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks.Description": {
      "FDescription": {
        "English": "Costomize your glow-y little friends!\n\nAllows you to change the built in glowstick type and/or customize the color to your liking, or color it based on the player who threw the glowstick.",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "定制你的荧光小伙伴！\n\n允许你更改内置荧光棒的类型和/或根据自己的喜好自定义颜色，或者根据投掷荧光棒的玩家来着色。"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks+GlowstickSettings.Override": {
      "FSDisplayName": {
        "English": "Override",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "覆盖"
      },
      "FSDescription": {
        "English": "Should any thrown glowsticks type be changed?",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "任何投掷荧光棒的类型应该被改变吗?"
      },
      "FSHeader": {
        "English": "://Synced to other players",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "://同步到其他玩家"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks+GlowstickSettings.OverrideType": {
      "FSDisplayName": {
        "English": "Override Color",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "覆盖颜色"
      },
      "FSDescription": {
        "English": "The color overriden glowsticks should be.\n(enable setting above!)\n({nameof(GlowstickType.Orange)} is only available in R5 and later!)\n({nameof(GlowstickType.Yellow)} is only available on ALT://R1 and later!)",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "荧光棒的颜色应该是。\n(启用上述设置!)\n({nameof(GlowstickType.Orange)} 仅在R5和更高版本中可用!)\n({nameof(GlowstickType.Yellow)} 仅在ALT://R1和更高版本上可用!)"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks+GlowstickSettings.AutoFestiveGlowsticks": {
      "FSDisplayName": {
        "English": "Auto Festive Glowsticks",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "自动切换节日荧光棒"
      },
      "FSDescription": {
        "English": "Automatically switch to festive versions on holidays.\n(<#F00>XMas => Red</color>)\n(<color=orange>Halloween => Orange</color>)",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "在节假日自动切换到节日版本。\n(<#F00>XMas => Red</color>)\n(<color=orange>Halloween => Orange</color>)"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks+GlowstickSettings.Mode": {
      "FSDisplayName": {
        "English": "Local Color",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "本地颜色"
      },
      "FSDescription": {
        "English": "Change colors for you only\n\n<#404>{nameof(LocalOverrideMode.Default)}</color>: Do whatever the game does\n<#404>{nameof(LocalOverrideMode.ForceColor)}</color>: Force Glowstick Colors\n<#404>{nameof(LocalOverrideMode.UsePlayerColor)}</color>: Player color determines Glowstick color\n\n<color=orange>(NOTICE: The first time per game start, after throwing the first glowstick, the game might stutter a little for a second to generate a texture)</color>",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "只改变你的颜色\n\n<#404>{nameof(LocalOverrideMode.Default)}</color>: 跟随游戏设定\n<#404>{nameof(LocalOverrideMode.ForceColor)}</color>: 强制荧光棒的颜色\n<#404>{nameof(LocalOverrideMode.UsePlayerColor)}</color>: 玩家的颜色决定荧光棒的颜色\n\n<color=orange>(注意:每次游戏开始时，在投掷第一支荧光棒后，游戏可能会在生成纹理时停顿一秒钟)</color>"
      },
      "FSHeader": {
        "English": "://Locally visible only settings",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "://仅在本地可见的设置"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks+GlowstickSettings.ForcedColor": {
      "FSDisplayName": {
        "English": "Force Color",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "强制改变颜色"
      },
      "FSDescription": {
        "English": "Sets the Glowstick color for every glowstick\nSet Mode above to {nameof(LocalOverrideMode.ForceColor)} to use this!",
        "French": null,
        "Italian": null,
        "German": null,
        "Spanish": null,
        "Russian": null,
        "Portuguese_Brazil": null,
        "Polish": null,
        "Japanese": null,
        "Korean": null,
        "Chinese": "设置每个荧光棒的颜色\n将上面的模式设置为 {nameof(LocalOverrideMode.ForceColor)} 用这个!"
      }
    },
    "TheArchive.Features.Cosmetic.Glowsticks+GlowstickSettings.ScaleUpColors": {
      "FSDisplayName": {
        "English": "Scale up dark colors",
        "French": null,
        "Italian": null,
